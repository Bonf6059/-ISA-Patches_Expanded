<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Caravan -->
    
<TraderKindDef>
    <defName>ISA_Caravan_EssentialsSupplier</defName>
    <label>essentials supplier</label>
    <stockGenerators>
        
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>2500~4500</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineHerbal</thingDef>
        <countRange>10~20</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineIndustrial</thingDef>
        <countRange>5~20</countRange>
      </li>

      <li Class="StockGenerator_SingleDef">
        <thingDef>Chocolate</thingDef>
        <countRange>-5~12</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>Beer</thingDef>
        <countRange>-3~6</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>Chemfuel</thingDef>
        <countRange>-300~150</countRange>
      </li>

      <li Class="StockGenerator_Category">
        <categoryDef>Textiles</categoryDef>
        <excludedThingDefs>
          <li>Leather_Human</li>
          <li>Leather_Patch</li>
          <li>Synthread</li>
          <li>Hyperweave</li>
        </excludedThingDefs>
        <thingDefCountRange>1~5</thingDefCountRange>
        <totalPriceRange>100~775</totalPriceRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>5~15</countRange>
      </li>
      
      <li Class="StockGenerator_Category">
        <categoryDef>FoodRaw</categoryDef>
        <excludedCategories>
          <li>EggsFertilized</li>
        </excludedCategories>
        <thingDefCountRange>3~3</thingDefCountRange>
        <totalPriceRange>150~800</totalPriceRange>
      </li>
      
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
      </li>
      
      <li Class="StockGenerator_Clothes">
        <countRange>10~10</countRange>
      </li>
      
      <!-- Buying -->
      
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      
    </stockGenerators>
  </TraderKindDef>
    
